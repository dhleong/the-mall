{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "lib": [
            "dom",
            "es2015",
            "es2016"
        ],
        "outDir": "dist",
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ],
        "declaration": true,
        "noUnusedLocals": true
    },
    "include": [
        "lib/**/*"
    ]
}
